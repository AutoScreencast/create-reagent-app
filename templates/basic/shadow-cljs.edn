{:source-paths ["src"]
 :dependencies [[reagent "0.10.0"]]
 :dev-http     {3000 "public"}
 :builds       {:app  {:target     :browser
                       :output-dir "public/js"
                       :asset-path "/js"
                       :modules    {:main {:init-fn *|USER-PROJECT-NAME|*.app.core/main}}}
                :test {:target     :node-test
                       :output-to  "out/node-tests.js"
                       :autorun    true}}}
